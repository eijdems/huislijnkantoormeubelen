<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
  <acl>
     <resources>
        <resource id="Magento_Backend::admin">
           <resource id="FME_Extensions::fmeextensions" title="FME Extensions" sortOrder="70">
              <resource id="FME_Productattachments::fmeextensions_productattachments" title="Product Attachments" sortOrder="80">
                 <resource id="FME_Productattachments::fmeextensions_productattachments_items" title="Manage Attachments" sortOrder="80"/>
                 <resource id="FME_Productattachments::fmeextensions_productattachments_ext" title="Manage File Extensions" sortOrder="85"/>
                 <resource id="FME_Productattachments::fmeextensions_productattachments_downloadlog" title="Manage Download Logs" sortOrder="86"/>
                 <resource id="FME_Productattachments::fmeextensions_productattachments_productcats" title="Manage Categories" sortOrder="90"/>
                 <resource id="FME_Productattachments::fmeextensions_productattachments_import" title="Import Attachments" sortOrder="100"/>
              </resource>
           </resource>
            <resource id="Magento_Backend::stores">
              <resource id="Magento_Backend::stores_settings">
                <resource id="Magento_Config::config">
                    <resource id="FME_Productattachments::fmeextensions_productattachments_config" title="Attachments Section"/>
                 </resource>
              </resource>
           </resource>
        </resource>
     </resources>
  </acl>
</config>